{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "email-frontend",
            "image": "vrushabhgore/email-frontend",
            "hostname": "frontend",
            "essential": false
        },{
            "name": "email-api",
            "image": "vrushabhgore/email-api",
            "hostname": "api",
            "essential": false
        },{
            "name": "nginx",
            "image": "vrushabhgore/nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":["email-frontend","email-api"]
        }
    ]
}